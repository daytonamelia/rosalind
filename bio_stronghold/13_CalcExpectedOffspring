def expected_offspring(domdom, domhet, domrec, hethet, hetrec, recrec):
    offspring = [
        domdom * 1.00 * 2,
        domhet * 1.00 * 2,
        domrec * 1.00 * 2,
        hethet * 0.75 * 2,
        hetrec * 0.50 * 2,
        recrec * 0.00 * 2,
    ]
    return sum(offspring)

expected_offspring(16307, 19930, 18816, 17004, 16314, 18320)